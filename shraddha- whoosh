void setup(){
  size(500,500);
  background(0);
}

void draw(){
  stroke(#11FFEE);
  noFill();
  arc(250, 250, 100, 100, PI, TWO_PI);
  arc(250, 250, 75, 75, PI, TWO_PI-(PI/6));
  arc(250, 250, 50, 50, PI, TWO_PI-(PI/4));
  arc(250, 250, 25, 25, PI, TWO_PI-(PI/2));
  colorMode(RGB);
  resetMatrix();
}
